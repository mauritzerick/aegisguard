{
  "name": "aegis-api",
  "version": "0.1.0",
  "private": true,
  "main": "dist/main.js",
  "scripts": {
    "start": "node dist/main.js",
    "start:dev": "ts-node-dev --respawn --transpile-only --files src/main.ts",
    "build": "tsc -p tsconfig.build.json",
    "prisma:migrate": "cd ../.. && npx prisma migrate dev --name init --schema prisma/schema.prisma && cd apps/api",
    "prisma:generate": "cd ../.. && npx prisma generate --schema prisma/schema.prisma && cd apps/api",
    "seed": "cd ../.. && npx ts-node prisma/seed.ts && cd apps/api"
  },
  "dependencies": {
    "@nestjs/common": "^10.4.7",
    "@nestjs/config": "^3.2.2",
    "@nestjs/core": "^10.4.7",
    "@nestjs/jwt": "^10.0.0",
    "@nestjs/platform-express": "^10.4.7",
    "@nestjs/platform-ws": "^10.4.7",
    "@nestjs/websockets": "^10.4.7",
    "@nestjs/swagger": "^7.4.2",
    "@prisma/client": "^5.20.0",
    "argon2": "^0.41.1",
    "bullmq": "^5.12.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cookie-parser": "^1.4.6",
    "date-fns": "^3.0.0",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^7.4.0",
    "helmet": "^7.1.0",
    "ioredis": "^5.4.1",
    "ip-cidr": "^3.1.0",
    "jsonwebtoken": "^9.0.2",
    "node-cron": "^3.0.3",
    "otplib": "^12.0.1",
    "pino-http": "^10.3.0",
    "qrcode": "^1.5.4",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "uuid": "^9.0.1",
    "ws": "^8.18.0",
    "zod": "^3.23.8",
    "@clickhouse/client": "^0.3.0",
    "pg": "^8.11.3",
    "geoip-lite": "^1.4.10",
    "ua-parser-js": "^1.0.37",
    "crypto": "^1.0.1"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.7",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.11.30",
    "@types/node-cron": "^3.0.11",
    "@types/qrcode": "^1.5.5",
    "@types/pg": "^8.11.0",
    "@types/ua-parser-js": "^0.7.39",
    "@types/ws": "^8.5.12",
    "prisma": "^5.20.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.5"
  }
}
